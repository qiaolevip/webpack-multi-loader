{
  "name": "webpack-multi-loader",
  "version": "1.0.0",
  "description": "This webpack project teach you how to use webpack loader in Webpack.",
  "keywords": [
    "react",
    "multi",
    "loader",
    "webpack"
  ],
  "main": "app/index.html",
  "scripts": {
    "start": "webpack-dev-server --progress --colors --no-info --history-api-fallback --inline --port 1377 --content-base ./app",
