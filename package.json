    "build": "gulp build --type production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qiaolevip/webpack-multi-loader.git"
  },
  "author": "@HappyJoe <qiaole@vip.qq.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/qiaolevip/webpack-multi-loader/issues"
  },
  "homepage": "https://github.com/qiaolevip/webpack-multi-loader#readme",
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.0",
    "babel-preset-stage-1": "^6.5.0",
    "css-loader": "^0.23.1",
    "json-loader": "^0.5.4",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
